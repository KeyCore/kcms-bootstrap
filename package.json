{
  "name": "kcms-bootstrap",
  "version": "0.1.0",
  "license": "UNLICENCED",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf cdk.out ts-build",
    "synth": "npm run build; npm run cdk synth -- -q",
    "deploy": "npm run cdk deploy -- --require-approval never",
    "cdk:bootstrap": "npm run cdk bootstrap -- --app '' --qualifier kcms --toolkit-stack-name KCMSCDKToolkit --cloudformation-execution-policies arn:aws:iam::aws:policy/AdministratorAccess",
    "kcms:deploy-bootstrap": "npm run deploy KCMSBootstrapStack --context",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.4.8",
    "aws-cdk": "^2.90.0",
    "jest": "^29.6.2",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.90.0",
    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
  }
}
